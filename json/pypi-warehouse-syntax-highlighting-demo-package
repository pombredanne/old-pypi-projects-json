  exit 2\n    fi\n\n    someRandomFunc(){\n     for i in $(ls -la ~/); do cat $i; done;\n     echo -e \"# I'm in ${HOME_DIR}$1/$2 :\"\n    }\n\n\nChangelog\n=========\n\n0.1.0 (2018-08-29)\n------------------\n\n* First version of syntax highlighting example package",
